<div class="content">
    <div class="container">
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 mb40 text-center">
                <h1>Artist Search</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 text-left">
                <h5>Select Your Prefences</h5>
            </div>
        </div>
        <div class="row" id="catFeed">

            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">


                        <input class="single-checkbox" type="radio" name="category" value="American Traditional">
                        <img class="card-img-top" src="/images/icons/amerTrad-sm.jpg" alt="American Traditional">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="Lettering">
                        <img class="card-img-top" src="/images/icons/lettering-sm.jpg">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="New School">

                        <img class="card-img-top" src="/images/icons/newSchool-sm.jpg">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="Realism">

                        <img class="card-img-top" src="/images/icons/realism-sm.jpg">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="Watercolor">

                        <img class="card-img-top" src="/images/icons/watercolor-sm.jpg">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="Portraiture">

                        <img class="card-img-top" src="/images/icons/port-sm.jpg">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="Blackwork">

                        <img class="card-img-top" src="/images/icons/blackwork-sm.jpg">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="Geometric">

                        <img class="card-img-top" src="/images/icons/geometric-sm.jpg">
                    </div>
                </label>
            </div>
            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <input class="single-checkbox" type="radio" name="category" value="Illustrative">

                        <img class="card-img-top" src="/images/icons/illustrative-sm.jpg">
                    </div>
                </label>
            </div>
        </div>
        <div class="row align-middle">
            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 mb40 text-center searchrow2">
                <div class="radioSelect">
                    <label>
                        <input type="radio" class="radio-inline" name="colorChoice" value="1">
                        <span class="outside">
                            <span class="inside"></span>
                        </span>Color</label>
                    <label>
                        <input type="radio" class="radio-inline" name="colorChoice" value="0">
                        <span class="outside">
                            <span class="inside"></span>
                        </span>Black & White</label>
                    <label>
                        <input type="radio" class="radio-inline" name="colorChoice" value="2">
                        <span class="outside">
                            <span class="inside"></span>
                        </span>Either</label>
                </div>
            </div>
            <div id="searchSubmitContainer" class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 text-center">
                <button id="searchSubmit" type="submit" class="btn btn-lg btn-block">Search</button>
            </div>
        </div>

        <div class="row resultsBG">

            <div class="col-xl-2 col-lg-2 col-md-6 col-sm-12 col-12 text-center">
                <div id="img-container">
                    <div class="ratio img-responsive img-circle" style="background-image: url(https://www.bspmediagroup.com/event/img/logos/user_placeholder.png);">
                    </div>
                </div>
                <h5 class="space">{{users.0.email}}</h5>
                <div class="lessDark" id="pills">
                    <span class="badge badge-pill">Color</span>
                    <span class="badge badge-pill">American Traditional</span>
                    <span class="badge badge-pill">Realism</span>
                    <span class="badge badge-pill">Lettering</span>
                </div>
            </div>

            <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
