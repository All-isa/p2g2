                    </div>
                </label>
            </div>

            <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <img class="card-img-top" src="http://via.placeholder.com/250x250">
                    </div>
                </label>
            </div>


            <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12 col-12">
                <!-- team-img -->
                <label>
                    <div class="card">

                        <img class="card-img-top" src="http://via.placeholder.com/250x250">
                    </div>
                </label>
            </div>

            <div class="col-xl-1 col-lg-1 col-md-1 col-sm-12 col-12 ">
                <a href="/about" class="btn btn-lg btn-block"><i class="fas fa-arrow-right"></i></a>
            </div>

        </div>
    </div>
